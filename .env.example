# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Demo Mode (set to 'true' to use localStorage instead of Supabase)
NEXT_PUBLIC_USE_LOCALSTORAGE=false

# Cloudinary Configuration (Optional - for image optimization)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name

# Resend Email Configuration (Optional - for email notifications)
RESEND_API_KEY=your_resend_api_key

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
